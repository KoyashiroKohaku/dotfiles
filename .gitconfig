[include]
	path = .gitconfig.user
[core]
	editor = vim -c 'set fenc=utf-8'
	autoCRLF = false
	quotepath = false
	pager = LESSCHARSET=utf-8 less
[merge]
	ff = false
[push]
	default = current
[pull]
	ff = only
[color]
	ui = auto
[alias]
	st = status
	sts = status --short
	co = checkout
	br = branch
	cm = commit
	rb = rebase
	mg = merge
	cp = cherry-pick
[diff]
	tool = vimdiff
[difftool]
	prompt = false
