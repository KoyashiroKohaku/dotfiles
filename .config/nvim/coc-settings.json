{
  "suggest.noselect": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "markdown",
    "json",
    "yaml",
    "toml",
    "rust",
    "go",
    "javascript",
    "typescript"
  ],
  "eslint.options": {
    "configFile": "$XDG_CONFIG_HOME/eslint/global.eslintrc.yml"
  },
  "eslint.nodePath": "$XDG_DATA_HOME/npm/bin/eslint"
}
